#define N 10
#include <stdio.h>
int main()
{
	int array[N] = {2,7,3,1,8,0,0,6,4,5};
	int bucket[N] = {0};
	int i,j=0;
	
	for(i=0;i<N;i++) // input bucket function
	{
		bucket[array[i]] += 1;
	}
	
	for(i=0;i<N;i++) // bucket ouput
	{
		while(bucket[i])
		{
			array[j] = i;
			bucket[i] -= 1;
			j++;
		}
	}
	
	for(i=0;i<N;i++) // printing the orderd array
	{
		if(array[i] != 0;
		printf("%d",array[i]);
	}
}
